<%= form_with model: @list do |form| %>
  <div class="field">
    <%= form.label :list_name %><br>
    <%= form.text_field :list_name %>
    <% @list.errors.full_messages_for(:list_name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div class="actions">
    <%= form.submit 'Create List' %>
  </div>

<% end %>